pipeline{
  agent any

  stages{
    stage('Checkout'){
      steps{
        //소스코드 체크아웃
        checkout scm
      }
    }
    stage('Build'){
      steps{
        //Java 파일들을 컴파일하여 생성된 클래스 파일을 classes 디렉토리에 저장
        //window일 경우 bat
        sh 'javac -encoding UTF-8 -d classes Practice/src/**/*.java'
